<h2>New User</h2>
<%= form_for :user , :url => users_path do |f| %>
    <p>
      <%= f.label :name %><br>
      <%=f.text_field :name %>
    </p>
    <p>
      <%= f.label :email %> <br>
      <%= f.text_field :email %>
     </p>
    <p>
      <%=f.label :password %> <br>
      <%= f.password_field :password %>
    </p>

    <p>
      <%=f.label :hobbies %> <br>
      <%=f.text_area :hobbies %>
    </p>



    <p>
      <%= f.submit %>
    </p>
<% end %>
<%= link_to 'Back', users_path %>